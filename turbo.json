{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": [
        "^start"
      ]
    },
    "pre-build": {
      "dependsOn": [
        "^pre-build"
      ]
    }
  }
}