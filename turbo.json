{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    }
  }
}