{
	"$schema": "https://json.schemastore.org/package",
	"name": "@cuicui/hooks",
	"version": "0.0.0",
	"private": true,
	"main": "index.ts",
	"types": "index.ts",
	"exports": {
		".": "./index.ts"
	},
	"scripts": {
		"_build": "pnpm pre-build",
		"pre-build": "pnpm generate-preview-categories && pnpm generate-section-list",
		"type-check": "tsc --noEmit",
		"generate-preview-categories": "deno run --allow-read --allow-write scripts/generate-preview-categories.ts && pnpm biome format --write ./categories-previews-list.ts",
		"generate-section-list": "deno run --allow-read --allow-write scripts/generate-section-list/main.ts && pnpm biome format --write ./section-list.ts",
		"registry:build": "shadcn build"
	},
	"license": "MIT",
	"peerDependencies": {
		"clsx": "^2.1.1",
		"tailwind-merge": "^3"
	},
	"devDependencies": {
		"@cuicui/config-typescript": "workspace:*",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@types/web-bluetooth": "^0.0.21",
		"@vitejs/plugin-react": "4.7.0",
		"shiki": "3.9.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"vitest": "3.2.4"
	},
	"dependencies": {
		"@ctrl/tinycolor": "4.1.0",
		"@number-flow/react": "^0.5.10",
		"@radix-ui/react-accordion": "1.2.11",
		"@radix-ui/react-context-menu": "^2.2.15",
		"@radix-ui/react-dropdown-menu": "2.1.15",
		"@radix-ui/react-slider": "^1.3.5",
		"@radix-ui/react-tabs": "^1.1.12",
		"@uiw/react-signature": "1.3.2",
		"bezier-editor": "1.0.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "2.1.1",
		"dom-to-image-more": "3.6.0",
		"embla-carousel-react": "8.6.0",
		"get-similar-color": "1.1.4",
		"jsbarcode": "3.12.1",
		"lucide-react": "^0.536.0",
		"motion": "^12.23.12",
		"next": "15.4.5",
		"next-themes": "0.4.6",
		"qrcode.react": "4.2.0",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"shadcn": "2.10.0",
		"sonner": "^2.0.6",
		"tts-react": "4.1.0"
	}
}
