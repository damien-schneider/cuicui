{
	"$schema": "http://json.schemastore.org/tsconfig",
	"extends": "@cuicui/config-typescript/react-library.json",
	"include": ["."],
	"exclude": ["dist", "build", "node_modules"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"],
			"@/cuicui/hooks/*": ["./src/*"]
		},
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"outDir": "dist"
	}
}
